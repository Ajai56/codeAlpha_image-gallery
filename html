<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Responsive Image Gallery</title>
    <style>
        :root {
            --gap: 10px;
            --thumb-size: 150px;
            --bg-dark: rgba(0, 0, 0, 0.85);
            --transition: 0.3s ease;
        }

        * {
            box-sizing: border-box;
        }

        body {
            margin: 0;
            font-family: Arial, Helvetica, sans-serif;
            background: #f5f5f5;
            color: #222;
        }

        header {
            padding: 1rem 0.5rem;
            text-align: center;
        }

        h1 {
            margin: 0 0 1rem;
            font-size: 1.75rem;
        }

        /* ---------- FILTER BUTTONS ---------- */
        .filter-bar {
            display: flex;
            justify-content: center;
            flex-wrap: wrap;
            gap: 0.5rem;
        }

        .filter-bar button {
            padding: 0.5rem 1rem;
            border: none;
            border-radius: 4px;
            background: #ddd;
            cursor: pointer;
            transition: background var(--transition), color var(--transition);
        }

        .filter-bar button:hover,
        .filter-bar button.active {
            background: #333;
            color: #fff;
        }

        /* ---------- GALLERY ---------- */
        .gallery {
            display: grid;
            gap: var(--gap);
            grid-template-columns: repeat(auto-fill, minmax(var(--thumb-size), 1fr));
            padding: 0 1rem 2rem;
        }

        .gallery img {
            width: 100%;
            height: 100%;
            object-fit: cover;
            aspect-ratio: 1 / 1;
            border-radius: 8px;
            cursor: pointer;
            transition: transform var(--transition), box-shadow var(--transition);
        }

        .gallery img:hover {
            transform: scale(1.05);
            box-shadow: 0 4px 15px rgba(0, 0, 0, 0.2);
        }

        /* ---------- LIGHTBOX ---------- */
        .lightbox {
            position: fixed;
            inset: 0;
            background: var(--bg-dark);
            backdrop-filter: blur(2px);
            display: flex;
            justify-content: center;
            align-items: center;
            opacity: 0;
            visibility: hidden;
            transition: opacity var(--transition);
            z-index: 1000;
        }

        .lightbox.open {
            opacity: 1;
            visibility: visible;
        }

        .lightbox img {
            max-width: 90vw;
            max-height: 90vh;
            border-radius: 8px;
            box-shadow: 0 2px 20px rgba(0, 0, 0, 0.4);
        }

        .lightbox .controls {
            position: absolute;
            top: 50%;
            translate: 0 -50%;
            width: 100%;
            display: flex;
            justify-content: space-between;
            pointer-events: none; /* avoid intercepting backdrop click */
        }

        .lightbox button {
            background: none;
            border: none;
            font-size: 3rem;
            color: #fff;
            cursor: pointer;
            pointer-events: auto;
            padding: 0 1rem;
            transition: opacity var(--transition);
        }

        .lightbox button:hover {
            opacity: 0.7;
        }

        .lightbox .close-btn {
            position: absolute;
            top: 20px;
            right: 25px;
            font-size: 2.25rem;
        }

        /* ---------- RESPONSIVE TWEAKS ---------- */
        @media (min-width: 768px) {
            :root {
                --thumb-size: 200px;
            }
        }
    </style>
</head>
<body>
    <header>
        <h1>My Image Gallery</h1>
        <div class="filter-bar">
            <button class="filter-btn active" data-filter="all">All</button>
            <button class="filter-btn" data-filter="nature">Nature</button>
            <button class="filter-btn" data-filter="city">City</button>
            <button class="filter-btn" data-filter="animals">Animals</button>
        </div>
    </header>

    <main>
        <div id="gallery" class="gallery">
            <img src="data:image/jpeg;base64," alt="Waterfall" data-category="nature" />
            <img src="data:image/jpeg;base64," alt="City at night" data-category="city" />
            <img src="data:image/jpeg;base64," alt="Tiger" data-category="animals" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQe7hVf5ruzAVdgMKsk5YADcuF30Xj2GefX7g&s" alt="Mountain" data-category="nature" />
            <img src="data:image/jpeg;base64," alt="Bridge" data-category="city" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQS94juQHl71R2J9WCUnh7b1Cj4pUcgpVpgiA&s" alt="Bird" data-category="animals" />
            <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcS8QsgRm06BKmcCdig-iW_CZwo-UlPsv105cw&s" alt="Forest" data-category="nature" />
            <img src="data:image/jpeg;base64," alt="Building" data-category="city" />
            <img src="data:image/jpeg;base64," alt="Dog" data-category="animals" />
        </div>
    </main>

    <!-- ---------- LIGHTBOX ---------- -->
    <div id="lightbox" class="lightbox" aria-hidden="true">
        <button class="close-btn" aria-label="Close">&times;</button>
        <div class="controls">
            <button class="prev-btn" aria-label="Previous">&#10094;</button>
            <button class="next-btn" aria-label="Next">&#10095;</button>
        </div>
        <img src="" alt="Expanded view" />
    </div>

    <!-- ---------- SCRIPTS ---------- -->
    <script>
        const gallery = document.getElementById("gallery");
        const lightbox = document.getElementById("lightbox");
        const lightboxImg = lightbox.querySelector("img");
        const prevBtn = lightbox.querySelector(".prev-btn");
        const nextBtn = lightbox.querySelector(".next-btn");
        const closeBtn = lightbox.querySelector(".close-btn");
        const filterButtons = document.querySelectorAll(".filter-btn");

        let currentIndex = 0;
        let images = [...gallery.querySelectorAll("img")];

        /* ---------- LIGHTBOX OPEN ---------- */
        gallery.addEventListener("click", (e) => {
            if (e.target.tagName !== "IMG") return;

            // refresh list, in case filter changed
            images = [...gallery.querySelectorAll("img")].filter(
                (img) => img.style.display !== "none"
            );

            currentIndex = images.indexOf(e.target);
            openLightbox();
        });

        function openLightbox() {
            updateLightbox();
            lightbox.classList.add("open");
            lightbox.setAttribute("aria-hidden", "false");
        }

        function closeLightbox() {
            lightbox.classList.remove("open");
            lightbox.setAttribute("aria-hidden", "true");
        }

        function updateLightbox() {
            const img = images[currentIndex];
            lightboxImg.src = img.src.replace(/400x400/, "1200x800"); 
            lightboxImg.alt = img.alt;
        }

        prevBtn.addEventListener("click", () => {
            currentIndex = (currentIndex - 1 + images.length) % images.length;
            updateLightbox();
        });

        nextBtn.addEventListener("click", () => {
            currentIndex = (currentIndex + 1) % images.length;
            updateLightbox();
        });

        closeBtn.addEventListener("click", closeLightbox);

        // close when clicking dark backdrop
        lightbox.addEventListener("click", (e) => {
            if (e.target === lightbox) closeLightbox();
        });

        // keyboard navigation
        document.addEventListener("keydown", (e) => {
            if (!lightbox.classList.contains("open")) return;
            if (e.key === "ArrowLeft") prevBtn.click();
            else if (e.key === "ArrowRight") nextBtn.click();
            else if (e.key === "Escape") closeLightbox();
        });

        /* ---------- FILTERS ---------- */
        filterButtons.forEach((btn) => {
            btn.addEventListener("click", () => {
                filterButtons.forEach((b) => b.classList.remove("active"));
                btn.classList.add("active");

                const filter = btn.dataset.filter;
                [...gallery.children].forEach((img) => {
                    img.style.display =
                        filter === "all" || img.dataset.category === filter
                            ? ""
                            : "none";
                });
            });
        });

        /* ---------- LAZY LOADING ---------- */
        if ("loading" in HTMLImageElement.prototype) {
            [...gallery.querySelectorAll("img")].forEach((img) => {
                img.loading = "lazy";
            });
        }
    </script>
</body>
</html>
